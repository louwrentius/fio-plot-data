[iotest]
rw=${RW}
blocksize=${BLOCKSIZE}
filename=${FILE}
ioengine=libaio
iodepth=${IODEPTH}
direct=1
invalidate=1
refill_buffers=1
numjobs=${NUMJOBS}
rwmixread=50
write_bw_log=${RW}-iodepth-${IODEPTH}-numjobs-${NUMJOBS}
write_lat_log=${RW}-iodepth-${IODEPTH}-numjobs-${NUMJOBS}
write_iops_log=${RW}-iodepth-${IODEPTH}-numjobs-${NUMJOBS}
log_avg_msec=1000

